{
  "name": "@talkohavy/filters",
  "version": "2.0.1",
  "description": "This package exposes an easy & intuitive filtering mechanism",
  "private": true,
  "license": "MIT",
  "keywords": ["filter", "talkohavy"],
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.esm.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.cjs.cjs"
      }
    }
  },
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "biome format ./src",
    "format-fix": "biome format --verbose ./src --write",
    "tsc": "tsc -p tsconfig.json",
    "format-prettier": "prettier . --check",
    "format-prettier-fix": "prettier . --write",
    "build": "node ./build.config.mjs",
    "test": "jest"
  },
  "contributors": ["Tal Kohavy <talkohavy@gmail.com> (@Tal Kohavy)"],
  "repository": {
    "type": "git",
    "url": "https://github.com/talkohavy/filters"
  },
  "author": "Tal Kohavy <talkohavy@gmail.com> (https://github.com/talkohavy)",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "restricted"
  },
  "bugs": {
    "url": "https://github.com/talkohavy/filters/issues",
    "email": "talkohavy@gmail.com"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@eslint/js": "^9.31.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "eslint": "^9.31.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "json-easy-strip": "^1.0.6",
    "prettier": "^3.6.2",
    "rollup": "^4.47.1",
    "ts-jest": "^29.4.1",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.10",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
