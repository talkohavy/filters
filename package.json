{
  "name": "@talkohavy/filters",
  "version": "1.0.34",
  "description": "This package exposes an easy & intuitive filtering mechanism",
  "private": true,
  "license": "MIT",
  "keywords": ["filter", "talkohavy"],
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.cjs"
      }
    }
  },
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "biome format ./src",
    "format-fix": "biome format --verbose ./src --write",
    "tsc": "tsc -p tsconfig.json",
    "format-prettier": "prettier . --check",
    "format-prettier-fix": "prettier . --write",
    "build": "node ./build.config.mjs",
    "test": "node --test"
  },
  "contributors": ["Tal Kohavy <talkohavy@gmail.com> (@Tal Kohavy)"],
  "repository": {
    "type": "git",
    "url": "https://github.com/talkohavy/filters"
  },
  "author": "Tal Kohavy <talkohavy@gmail.com> (https://github.com/talkohavy)",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "restricted"
  },
  "bugs": {
    "url": "https://github.com/talkohavy/filters/issues",
    "email": "talkohavy@gmail.com"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@changesets/cli": "^2.29.5",
    "@eslint/js": "^9.31.0",
    "@types/node": "^24.1.0",
    "eslint": "^9.31.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
